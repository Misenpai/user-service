generator client {
  provider      = "prisma-client-js"
  output        = "../generated/prisma"
  binaryTargets = ["native", "linux-musl", "linux-musl-openssl-3.0.x","debian-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL") // Make sure this points to your MySQL database
}

model User {
  id         Int    @id @default(autoincrement())
  username   String
  employeeId String
  empClass   String
  projectKey String
}
